{
  "cli": {
    "version": ">= 5.0.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "node": "18.20.4",
      "env": {
        "NODE_ENV": "production",
        "EXPO_USE_UNPACKED_PNPM": "1",
        "EXPO_USE_WORKSPACES": "1"
      },
      "cache": {
        "disabled": true
      },
      "ios": {
        "resourceClass": "m-medium"
      },
      "android": {
        "buildType": "apk"
      }
    },
    "preview": {
      "distribution": "internal",
      "node": "18.20.4",
      "env": {
        "NODE_ENV": "production",
        "EXPO_USE_UNPACKED_PNPM": "1",
        "EXPO_USE_WORKSPACES": "1"
      },
      "ios": {
        "resourceClass": "m-medium"
      },
      "android": {
        "buildType": "apk"
      }
    },
    "production": {
      "node": "18.20.4",
      "env": {
        "NODE_ENV": "production",
        "EXPO_USE_UNPACKED_PNPM": "1",
        "EXPO_USE_WORKSPACES": "1"
      },
      "ios": {
        "resourceClass": "m-medium"
      },
      "android": {
        "buildType": "app-bundle"
      }
    }
  },
  "submit": {
    "production": {}
  }
}